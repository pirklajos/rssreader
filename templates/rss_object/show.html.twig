{% extends 'base.html.twig' %}

{% block title %}RSSObject{% endblock %}

{% block body %}
    <h1>RSSObject</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ r_s_s_object.id }}</td>
            </tr>
            <tr>
                <th>Url</th>
                <td>{{ r_s_s_object.url }}</td>
            </tr>
            <tr>
                <th>Active</th>
                <td>{{ r_s_s_object.active ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_r_s_s_object_index') }}">back to list</a>

    <a href="{{ path('app_r_s_s_object_edit', {'id': r_s_s_object.id}) }}">edit</a>

    {{ include('rss_object/_delete_form.html.twig') }}
{% endblock %}
